CREATE TABLE stats(homeTeam TEXT, awayTeam TEXT, gameWeek TEXT, homeGoals INTEGER, awayGoals INTEGER, matchGoals INTEGER, BTTS TEXT, firstHalfHomeGoals INTEGER, firstHalfHomeConc INTEGER, firstHalfAwayGoals INTEGER, firstHalfAwayConc INTEGER, firstHalfTotalGoals INTEGER, secondHalfHomeGoals INTEGER, secondHalfHomeConc INTEGER, secondHalfAwayGoals INTEGER, secondHalfAwayConc INTEGER, secondHalfTotalGoals INTEGER,homeTeamCards INTEGER,awayTeamCards INTEGER,matchCards INTEGER,homeCorners INTEGER,awayCorners INTEGER,homeCornersConc INTEGER,awayCornersConc INTEGER,matchCorners INTEGER,league TEXT, gameID INTEGER)